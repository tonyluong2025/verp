{
    "label": "Code Backend Theme V1",
    "description": "Minimalist and elegant backend theme for Verp 1, Backend Theme, Theme",
    "summary": "Code Backend Theme V1 is an attractive theme for backend",
    "category": "Themes/Backend",
    "version": "1.0.0",
    "depends": ["base", "web", "mail"],
    "data": [
