<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="website.socialHover">
        <div class="text-nowrap css-editable-mode-hidden">
            <t t-foreach="medias" t-as="media">
                <a href="#"
                    t-attf-class="fa fa-3x fa-#{media}-square text-#{media} oe-social-#{media}"
                    t-att-title="media"
                    t-att-aria-label="media"/>
            </t>
        </div>
    </t>
</templates>
